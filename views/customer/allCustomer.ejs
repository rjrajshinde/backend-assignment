<h3><a class="btn btn-secondary float-right" href="/customer/addCustomer">Add Customer</a></h3>
<table class="table table-striped table-bordered">
    <thead class="thead-dark">
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Address</th>
            <th>Contact</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% data.forEach((customer)=> { %>
            <tr>
                <td>
                    <%- customer.name%>
                </td>
                <td>
                    <%- customer.email%>
                </td>
                <td>
                    <%- customer.address%>
                </td>
                <td>
                    <%- customer.contact%>
                </td>
                <td>
                    <a href="/customer/editCustomer/<%- customer._id%>" class="btn btn-warning table-btn">Edit</a>
                    <a href="/customer/deleteCustomer/<%- customer._id%>" class="btn btn-danger table-btn">Delete</a>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>